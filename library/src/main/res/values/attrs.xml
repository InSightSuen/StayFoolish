<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SquareLayout">
        <attr name="match" format="enum">
            <enum name="width" value="0"/>
            <enum name="height" value="1" />
            <enum name="length" value="2" />
        </attr>
        <attr name="length" format="dimension" />
    </declare-styleable>

    <declare-styleable name="PointProgress">
        <attr name="progress" format="integer|float" />
        <attr name="max" format="integer|float" />
        <attr name="stroke_width" format="dimension" />
        <attr name="paint_radius" format="dimension" />
        <attr name="background_color" format="color" />
        <attr name="primary_color" format="color" />
    </declare-styleable>
</resources>